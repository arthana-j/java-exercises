class Vowelcheck{
    public static void main(string args[]){
        char letter = 'I';

        Switch (letter){
            case 'A':
            System.out.println("Its a Vowel");
            break;
            case 'E':
            System.out.println("Its a Vowel");
            break;
            case 'I':
            System.out.println("Its a Vowel");
            break;
            case 'O':
            System.out.println("Its a Vowel");
            break;
            case 'U':
            System.out.println("Its a Vowel");
            break;
            default:
            System.out.println("Its not");
        }
    }
}